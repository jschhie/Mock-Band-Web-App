{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*\\.(?:css|js|gif|png|jpe?g|svg|ico|woff2?|eot|ttf|otf))",
      "headers": {
        "Cache-Control": "public, max-age=2592000, immutable"
      },
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}